# ⚠️ SECURITY WARNING: Never commit .env files with real credentials to Git!
# Copy this file to .env and fill in your actual values

# API Configuration
REACT_APP_API_ENDPOINT=https://your-api-id.execute-api.ap-northeast-2.amazonaws.com/dev
REACT_APP_API_BASE_URL=http://localhost:3001
REACT_APP_WS_URL=wss://ws.profilephotoai.com

# AWS Cognito Configuration (REQUIRED for production)
REACT_APP_COGNITO_USER_POOL_ID=ap-northeast-2_xxxxxxxxx
REACT_APP_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
REACT_APP_COGNITO_DOMAIN=your-cognito-domain
REACT_APP_AWS_REGION=ap-northeast-2
REACT_APP_REDIRECT_URI=http://localhost:3000/callback

# Development
REACT_APP_ENV=development
REACT_APP_ENVIRONMENT=development

